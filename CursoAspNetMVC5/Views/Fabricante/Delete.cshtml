@model CursoAspNetMVC5.Models.Fabricante

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Eliminar Frabricante</title>
</head>
<body>
    <h3>Você tem a certeza que deseja Eliminar o Fabricante?</h3>
    <div>
        <h4>Fabricante</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.Nome)
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Nome)
            </dd>
    
        </dl>
    
        @using (Html.BeginForm()) {
            @Html.AntiForgeryToken()
    
            <div class="form-actions no-color">
                <input type="submit" value="Eliminar" class="btn btn-default" /> |
                @Html.ActionLink("Voltar à Lista de Fabricantes", "Index")
            </div>
        }
    </div>
</body>
</html>
